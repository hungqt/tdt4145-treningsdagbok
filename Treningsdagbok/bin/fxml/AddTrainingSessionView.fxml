<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="AddExerciseController">
   <children>
      <Pane prefHeight="600.0" prefWidth="800.0">
         <children>
            <Text layoutX="180.0" layoutY="94.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Lag treningsøkt">
               <font>
                  <Font size="64.0" />
               </font>
            </Text>
            <TextField fx:id="tid" layoutX="325.0" layoutY="189.0" onAction="#handleTid" promptText="Tid (tt:mm)" />
            <DatePicker fx:id="dato" layoutX="313.0" layoutY="142.0" onAction="#handleDato" promptText="Dato" />
            <TextField fx:id="varighet" layoutX="326.0" layoutY="241.0" onAction="#handleVarighet" promptText="Varighet (tt:mm)" />
            <TextField fx:id="personligForm" layoutX="325.0" layoutY="288.0" onAction="#handlePersonligForm" promptText="Personlig form før økten" />
            <Button fx:id="btnLegOv" layoutX="338.0" layoutY="338.0" mnemonicParsing="false" onAction="#handleLeggTilØvelse" prefHeight="75.0" prefWidth="123.0" text="Legg til øvelser &gt;&gt;" />
         </children>
      </Pane>
   </children>
</AnchorPane>
